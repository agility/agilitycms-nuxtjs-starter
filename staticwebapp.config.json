{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/api/redirect",
    "exclude": [
      "/_nuxt/*",
      "/images/*",
      "/css/*",
      "/*.ico",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif",
      "/*.svg",
      "/*.js",
      "/*.css",
      "/*.woff",
      "/*.woff2",
      "/*.ttf",
      "/*.eot"
    ]
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}